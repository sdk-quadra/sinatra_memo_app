<h6>メモ内容変更</h6>
  <form method="post" action="/<%= params[:id] %>">
    <div class="form-group">
      <textarea name="memo" rows="5" class="form-control bg-gray"><%= @content %></textarea>
    </div>
    <div class="form-group">
      <input type="hidden" value="patch" name="_method">
      <input type="submit" value="保存" class="btn btn-success">
    </div>
  </form>
